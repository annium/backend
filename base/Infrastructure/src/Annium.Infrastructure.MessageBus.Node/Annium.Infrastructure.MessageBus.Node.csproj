<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>Annium.Infrastructure.MessageBus.Node</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Annium.Infrastructure.MessageBus.Node</Description>
        <TargetFramework>net8.0</TargetFramework>
        <OutputType>Library</OutputType>
        <DebugType>portable</DebugType>
        <WarningsAsErrors>true</WarningsAsErrors>
        <IsPackable>true</IsPackable>
        <Nullable>enable</Nullable>
        <NoWarn>$(NoWarn);NETSDK1206</NoWarn>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Annium.Data.Operations" Version="0.1.0" />
        <PackageReference Include="Annium.Extensions.Reactive" Version="0.1.0" />
        <PackageReference Include="Annium.Serialization.Json" Version="0.1.0" />
        <PackageReference Include="NetMQ" Version="4.0.1.6" />
        <PackageReference Include="System.Text.Json" Version="8.0.1" />
        <PackageReference Include="System.Threading.Channels" Version="8.0.0" />
    </ItemGroup>
</Project>