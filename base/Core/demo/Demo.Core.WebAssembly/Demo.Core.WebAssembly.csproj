<Project Sdk="Microsoft.NET.Sdk.Web">
    <PropertyGroup>
        <PackageId>Demo.Core.WebAssembly</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Annium.Wasm</Description>
        <TargetFramework>netstandard2.1</TargetFramework>
        <OutputType>Exe</OutputType>
        <DebugType>portable</DebugType>
        <LangVersion>latest</LangVersion>
        <WarningsAsErrors>true</WarningsAsErrors>
        <IsPackable>false</IsPackable>
        <Nullable>enable</Nullable>
        <RazorLangVersion>3.0</RazorLangVersion>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="../../src/Annium.Core.Runtime/Annium.Core.Runtime.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Annium.Core.DependencyInjection" Version="0.1.0" />
        <PackageReference Include="Annium.Net.Http" Version="0.1.0" />
        <PackageReference Include="BlazorStyled" Version="3.0.0" />
        <PackageReference Include="MatBlazor" Version="2.6.2" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="3.2.0" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Build" Version="3.2.0" />
        <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="3.2.0" />
        <PackageReference Include="System.Net.Http.Json" Version="3.2.0" />
    </ItemGroup>
</Project>