<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>Annium.Core.DependencyInjection</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Extension library for simplifying and powering up Microsoft DI container. Bundles and reuses services via ServicePack concept.</Description>
        <TargetFramework>net5.0</TargetFramework>
        <OutputType>Library</OutputType>
        <DebugType>portable</DebugType>
        <LangVersion>preview</LangVersion>
        <WarningsAsErrors>true</WarningsAsErrors>
        <IsPackable>true</IsPackable>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="../Annium.Core.Primitives/Annium.Core.Primitives.csproj" />
        <ProjectReference Include="../Annium.Core.Runtime/Annium.Core.Runtime.csproj" />
    </ItemGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection" Version="5.0.0" />
    </ItemGroup>
</Project>