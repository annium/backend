<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>Annium.Extensions.Arguments</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Extension library for building complex cli-based app. Provides entrypoint, arguments parsing and commands routing.</Description>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <OutputType>Library</OutputType>
        <DebugType>portable</DebugType>
        <WarningsAsErrors>true</WarningsAsErrors>
        <LangVersion>latest</LangVersion>
    </PropertyGroup>
    <ItemGroup>
        <ProjectReference Include="../../../Core/src/Annium.Core.DependencyInjection/Annium.Core.DependencyInjection.csproj" />
        <ProjectReference Include="../Annium.Extensions.Mapper/Annium.Extensions.Mapper.csproj" />
    </ItemGroup>
</Project>