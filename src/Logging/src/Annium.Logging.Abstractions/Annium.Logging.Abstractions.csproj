<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <PackageId>Annium.Logging.Abstractions</PackageId>
        <PackageVersion>0.1.0</PackageVersion>
        <Description>Logging abstractions library</Description>
        <TargetFramework>netcoreapp2.2</TargetFramework>
        <OutputType>Library</OutputType>
        <DebugType>portable</DebugType>
        <WarningsAsErrors>true</WarningsAsErrors>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Annium.Extensions.DependencyInjection" Version="0.1.0" />
        <PackageReference Include="NodaTime" Version="2.4.4" />
    </ItemGroup>
</Project>